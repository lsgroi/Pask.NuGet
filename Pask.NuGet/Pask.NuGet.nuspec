        <iconUrl>https://raw.githubusercontent.com/lsgroi/Pask/master/Pask.png</iconUrl>
        <summary>Pask extension which provides NuGet functionality to create and publish a package.</summary>
        <description>A Pask extension which provides NuGet functionality to create and publish a package.</description>
        <tags>Invoke-Build PowerShell BuildAutomation Pask NuGet</tags>
        <dependencies>
            <dependency id="Pask" version="0.14.1" />
        </dependencies>
    </metadata>
    <files>
        <file src="init\*" target="init" />
        <file src="scripts\*.ps1" target="scripts" exclude="scripts\*.Tests.ps1" />
        <file src="tasks\*.ps1" target="tasks" exclude="tasks\*.Tests.ps1" />
        <file src="tools\*.ps1" target="tools" />
        <file src="readme.txt" />
